CREATE TABLE russian_equipment_losses (
	id INTEGER PRIMARY KEY AUTOINCREMENT,
	date TEXT,
	days_past INTEGER,
	aircraft INTEGER,
	helicopters INTEGER,
	tanks INTEGER,
	APCs INTEGER,
	field_artillery INTEGER,
	MRL INTEGER,
	military_autos INTEGER,
	fuel_tanks INTEGER,
	drones INTEGER,
	naval_ships INTEGER,
	antiaircraft_warfare INTEGER,
	special_equipments INTEGER,
	mobile_SRBM_systems INTEGER,
	vehicles_fuel_tanks INTEGER,
	cruise_missles INTEGER,
	greatest_losses_direction TEXT
);

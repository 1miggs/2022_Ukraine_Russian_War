CREATE TABLE russian_personnel_losses (
	id INTEGER PRIMARY KEY AUTOINCREMENT,
	russian_equipment_losses_id INTEGER,
	date TEXT,
	days_past INTEGER,
	personnel INTEGER,
	approximation TEXT,
	pow INTEGER
);

---------------------------Query-----------------------------
/* Finding  50% of the max losses of helicopters */
SELECT 
	COUNT (*) 
FROM
	russian_equipment_losses
WHERE 
	helicopters >= ROUND(.90 * (140 -7)									

/* how many "about" estimates are there */
SELECT 
	COUNT(approximation) AS estimates_about
FROM
	russian_personnel_losses
WHERE
	approximation 
        LIKE "abo%";

/*how many "more" estimates are there */
SELECT 
	COUNT(approximation) AS estimates_more
FROM
	russian_personnel_losses
WHERE
    approximation IS NOT "about";

/* what date did drones over take helicopters in losses and its personnel */
SELECT
	rpl.date,
	rpl.day,
	rel.drone,
	rel.helicopter,
	rel.fuel tank,
	rel.mrl,
	rpl.personnel, 
	rpl.pow
FROM
	russian_equipment_losses AS rel
INNER JOIN 
	russian_personnel_losses AS rpl
ON
	rel.id = rpl.russian_equipment_losses_id
WHERE 
    drones > helicopters

/*the city of greatest losses up to date and the total of all losses*/
SELECT
	*
FROM
	russian_equipment_losses
GROUP BY
	greatest_losses_direction

/*first five days of the war seeing the dramatic loss increase */
SELECT
	rel.*,
	rpl.*,
FROM
	russian_equipment_losses AS rel
INNER JOIN
	russian_personnel_losses AS rpl
ON 
	rel.id = rpl.russian_equipment_losses_id
LIMIT 5

/*the most recent losses incurred by Russia */
SELECT
	rel.*,
	rpl.*
FROM
	russian_equipment_losses AS rel
INNER JOIN
	russian_personnel_losses AS rpl
ON 
	rel.id = rpl.russian_equipment_losses_id
ORDER BY 
	days_past DESC
LIMIT 7

------------------------Important info-----------------------
Acronyms
POW - Prisoner of War,
MRL - Multiple Rocket Launcher,
APC - Armored Personnel Carrier,
SRBM - Short-range ballistic missile,
drones:
UAV - Unmanned Aerial Vehicle,
RPA - Remotely Piloted Vehicle.
